{
  "manifest_version": 3,
  "name": "CodeSandbox Date Tags",
  "version": "1.0.2",
  "description": "Add date tags to CodeSandbox search results",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://codesandbox.io/*"
  ],
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "https://codesandbox.io/search*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
